@model AspNetCoreProject.Models.MenuModelManage

@{
    ViewData["Title"] = "Edit";
}

    <h2>Edit Menu Model Manage</h2>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="MenuModelId" />
            <div class="form-group">
                <div class="checkbox">
                    <label>
                        <input asp-for="Insert" /> @Html.DisplayNameFor(model => model.Insert)
                    </label>
                </div>
            </div>
            <div class="form-group">
                <div class="checkbox">
                    <label>
                        <input asp-for="Update" /> @Html.DisplayNameFor(model => model.Update)
                    </label>
                </div>
            </div>
            <div class="form-group">
                <div class="checkbox">
                    <label>
                        <input asp-for="Delete" /> @Html.DisplayNameFor(model => model.Delete)
                    </label>
                </div>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

